<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Signup.SignUpActivity">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="118.3dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="118.3dp"
        android:textStyle="normal"
        android:textSize="24sp"
        android:textColor="#303030"
        android:lineSpacingExtra="3.3sp"
        android:gravity="center"
        android:text="준비됐나요~" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="201.1dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:inputType="textPersonName"
        android:lineSpacingExtra="4.5sp"
        android:text="   닉네임"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp"
        android:textStyle="normal" />

    <EditText
        android:id="@+id/editTextTextEmailAddress3"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="253.5dp"
        android:layout_marginEnd="33dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:gravity="center_vertical"
        android:hint="   이메일"
        android:inputType="textEmailAddress"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp" />

    <EditText
        android:id="@+id/editTextTextPassword6"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="305.8dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:hint="   비밀번호(8자리이상/영어,숫자,특수문자)"
        android:inputType="textPassword"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp" />

    <EditText
        android:id="@+id/editTextTextPassword7"
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="358dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:hint="   비밀번호 재입력"
        android:inputType="textPassword"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp" />

    <Button
        android:id="@+id/button15"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="430.3dp"
        android:layout_marginEnd="34dp"
        android:layout_marginBottom="356dp"
        android:background="@drawable/round"
        android:textStyle="bold"
        android:text="회원가입" />


</RelativeLayout>
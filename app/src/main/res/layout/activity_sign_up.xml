<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Signup.RegisterActivity">

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="118.3dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="118.3dp"
        android:textStyle="bold"
        android:textSize="24sp"
        android:textColor="#303030"
        android:lineSpacingExtra="3.3sp"
        android:gravity="center"
        android:text="회원가입" />

    <EditText
        android:id="@+id/et_Nickname"
        android:layout_width="wrap_content"
        android:layout_height="44dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="180dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:hint="   닉네임"
        android:inputType="textPersonName"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp"
        android:textStyle="normal" />

    <TextView
        android:id="@+id/et_Nickname_warn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="228dp"
        android:text="! 닉네임은 최대 8글자 입니다."
        android:visibility="invisible"
        android:textColor="#fd9595"
        android:textSize="10dp" />

    <EditText
        android:id="@+id/et_Email"
        android:layout_width="wrap_content"
        android:layout_height="42dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="247dp"
        android:layout_marginEnd="33dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:gravity="center_vertical"
        android:hint="   이메일"
        android:inputType="textEmailAddress"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp" />

    <TextView
        android:id="@+id/et_Email_warn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="295dp"
        android:text="! 올바르지 않은 이메일입니다."
        android:visibility="invisible"
        android:textColor="#fd9595"
        android:textSize="10dp" />

    <EditText
        android:id="@+id/et_Passwd"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="33dp"
        android:layout_marginTop="307dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:hint="   비밀번호(8자리이상/영어,숫자,특수문자)"
        android:inputType="textPassword"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp" />

    <TextView
        android:id="@+id/et_Passwd_warn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="345dp"
        android:text="! 비밀번호는 8자리 이상, 영어, 숫자, 특수문자를 포함해야합니다."
        android:visibility="invisible"
        android:textColor="#fd9595"
        android:textSize="10dp" />

    <EditText
        android:id="@+id/et_Passwd2"
        android:layout_width="wrap_content"
        android:layout_height="43dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="33dp"
        android:layout_marginTop="367dp"
        android:layout_marginEnd="34dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:hint="   비밀번호 재입력"
        android:inputType="textPassword"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp" />

    <TextView
        android:id="@+id/et_Passwd2_warn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="40dp"
        android:layout_marginTop="415dp"
        android:text="! 비밀번호 재입력이 올바르지 않습니다."
        android:visibility="invisible"
        android:textColor="#fd9595"
        android:textSize="10dp" />

    <CheckBox
        android:id="@+id/privacy_check1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="436dp"
        android:text="개인정보 수집 및 이용 동의"
        android:textSize="12dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="32dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="207dp"
        android:layout_marginTop="436dp"
        android:gravity="center"
        android:text="(필수)"
        android:textColor="#fd9595"
        android:textSize="12dp"
        android:textStyle="bold" />

    <Button
        android:id="@+id/btn_register"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_centerHorizontal="true"
        android:layout_marginStart="35dp"
        android:layout_marginTop="480dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="356dp"
        android:background="@drawable/round"
        android:text="회원가입"
        android:textStyle="bold" />


</RelativeLayout>
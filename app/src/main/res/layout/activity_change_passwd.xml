<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".SettingPage.ChangePasswdActivity">

    <Button
        android:id="@+id/change_pw_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="376dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="356dp"
        android:background="@drawable/round"
        android:text="확인"
        android:textStyle="bold" />

    <EditText
        android:id="@+id/et_Passwd5"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="272dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:hint="   새로운 비밀번호(8-12자리/영어,숫자,특수문자)"
        android:inputType="textPassword"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp" />

    <EditText
        android:id="@+id/et_Passwd3"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="221dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:hint="   비밀번호"
        android:inputType="textPassword"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp" />

    <EditText
        android:id="@+id/et_Passwd4"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="324dp"
        android:layout_marginEnd="36dp"
        android:background="@drawable/round_white"
        android:ems="10"
        android:hint="   비밀번호 재입력"
        android:inputType="textPassword"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp" />

    <TextView
        android:id="@+id/et_Email3"
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="34dp"
        android:layout_marginTop="167dp"
        android:layout_marginEnd="36dp"
        android:ems="10"
        android:gravity="center_vertical"
        android:hint="   soooyl@daum.co.com"
        android:lineSpacingExtra="4.5sp"
        android:textColor="#a6a6a6"
        android:textSize="12.5sp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="39dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="101dp"
        android:layout_marginTop="99dp"
        android:layout_marginEnd="100.5dp"
        android:gravity="center"
        android:text="비밀번호 변경"
        android:textColor="@color/black"
        android:textSize="20sp"
        android:textStyle="bold" />
</RelativeLayout>
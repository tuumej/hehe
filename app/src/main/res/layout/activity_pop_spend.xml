<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".VS.PopActivitySpend">

    <CheckBox
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="323dp"
        android:layout_marginTop="131dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/ring1"
        android:text="월세" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="131dp"
        android:layout_height="37dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="130dp"
        android:layout_marginTop="44dp"
        android:layout_marginEnd="150dp"
        android:gravity="center"
        android:text="가계부 생성"
        android:textColor="@color/black"
        android:textSize="19.5sp"
        android:textStyle="bold" />

    <CheckBox
        android:id="@+id/texi"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="131dp"
        android:layout_marginEnd="318dp"
        android:background="@drawable/ring1"
        android:text="교통" />

    <CheckBox
        android:id="@+id/coffee"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="100dp"
        android:layout_marginTop="131dp"
        android:layout_marginEnd="245dp"
        android:background="@drawable/ring2"
        android:text="커피" />

    <CheckBox
        android:id="@+id/meal"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="173dp"
        android:layout_marginTop="131dp"
        android:layout_marginEnd="171dp"
        android:background="@drawable/ring3"
        android:text="식비" />

    <CheckBox
        android:id="@+id/life"
        android:layout_width="wrap_content"
        android:layout_height="34dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="249dp"
        android:layout_marginTop="131dp"
        android:layout_marginEnd="95dp"
        android:background="@drawable/ring4"
        android:text="생활" />

    <EditText
        android:id="@+id/payday"
        android:layout_width="352dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="215dp"
        android:layout_marginEnd="29dp"
        android:background="@drawable/round_search"
        android:ems="10"
        android:hint="  월급"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/pocketmoney"
        android:layout_width="352dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="267dp"
        android:layout_marginEnd="29dp"
        android:background="@drawable/round_search"
        android:ems="10"
        android:hint="  용돈"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/refund"
        android:layout_width="352dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="319dp"
        android:layout_marginEnd="29dp"
        android:background="@drawable/round_search"
        android:ems="10"
        android:hint="  환불"
        android:inputType="textPersonName" />

    <EditText
        android:id="@+id/ectmoney"
        android:layout_width="352dp"
        android:layout_height="40dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="370dp"
        android:layout_marginEnd="29dp"
        android:background="@drawable/round_search"
        android:ems="10"
        android:hint="  기타"
        android:inputType="textPersonName" />

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="98dp"
        android:text="지출"
        android:textColor="@color/black"
        android:textSize="17.5sp"
        android:textStyle="bold" />

</RelativeLayout>